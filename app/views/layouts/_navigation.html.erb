<%= link_to "Chatroom", root_path, class: "item" %>
<a class="item">
  Messages
</a>
<a class="item">
  Friends
</a>
<div class="right menu">
  <div class="item">
    <% if !logged_in? %>
      <%= link_to "Log In", login_path, class: "fluid ui primary button" %>
    <% else %>
      <%= link_to "Log Out", logout_path, class: "fluid ui negative button", method: :delete %>
    <% end %>
  </div>
</div>
